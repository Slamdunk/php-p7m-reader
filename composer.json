{
    "name": "slam/php-p7m-reader",
    "description": "P7M Reader",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-openssl": "*",
        "symfony/process": "^4.4 || ^5.2"
    },
    "require-dev": {
        "malukenho/mcbumpface": "^1.1.5",
        "phpstan/phpstan": "^0.12.80",
        "phpstan/phpstan-phpunit": "^0.12.17",
        "phpunit/phpunit": "^9.5.2",
        "slam/php-cs-fixer-extensions": "^2.2.0 || ^3.0.0",
        "slam/php-debug-r": "^1.7.0",
        "slam/phpstan-extensions": "^5.1.0"
    },
    "extra": {
        "mc-bumpface": {
            "stripVersionPrefixes": true
        }
    },
    "autoload": {
        "psr-4": {
            "Slam\\P7MReader\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slam\\P7MReader\\Tests\\": "tests/"
        }
    }
}
